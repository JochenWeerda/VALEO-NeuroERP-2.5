  "rawSource": "CREATE TABLE lager.chargen (\n    charge_id UUID PRIMARY KEY DEFAULT gen_random_uuid(),\n    artikel_id UUID NOT NULL,\n    lagerort_id UUID NOT NULL REFERENCES lager.lagerorte(lagerort_id),\n    lagerzone_id UUID REFERENCES lager.lagerzonen(lagerzone_id),\n    lagerplatz_id UUID REFERENCES lager.lagerplaetze(lagerplatz_id),\n    charge_nr VARCHAR(100) NOT NULL,\n    los_nr VARCHAR(100),\n    menge DECIMAL(10,4) NOT NULL,\n    einheit VARCHAR(20) NOT NULL,\n    herstellungsdatum DATE,\n    verfallsdatum DATE,\n    charge_status VARCHAR(20) DEFAULT 'VERF\u00dcGBAR' CHECK (charge_status IN ('VERF\u00dcGBAR', 'RESERVIERT', 'QUALITAET', 'GESPERRT', 'VERBRAUCHT')),\n    qualitaetsstatus VARCHAR(20) DEFAULT 'FREIGEGEBEN' CHECK (qualitaetsstatus IN ('FREIGEGEBEN', 'IN_PR\u00dcFUNG', 'GESPERRT', 'VERWORFEN')),\n    bemerkung TEXT,\n    erstellt_am TIMESTAMP DEFAULT CURRENT_TIMESTAMP,\n    geaendert_am TIMESTAMP DEFAULT CURRENT_TIMESTAMP\n);",
  "table": "lager.chargen"
