{
  "name": "@valero-neuroerp/scheduler-domain",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "tsx watch src/app/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.cjs",
    "clean": "rimraf dist coverage reports",
    "lint": "eslint src/**/*.ts tests/**/*.ts --ext .ts",
    "lint:fix": "eslint src/**/*.ts tests/**/*.ts --ext .ts --fix",
    "format": "prettier --write src/**/*.ts tests/**/*.ts",
    "format:check": "prettier --check src/**/*.ts tests/**/*.ts",
    "test": "vitest run --config vitest.config.ts",
    "test:watch": "vitest --config vitest.config.ts",
    "test:unit": "vitest run --config vitest.config.ts tests/unit",
    "test:integration": "vitest run --config vitest.config.ts tests/integration",
    "test:e2e": "vitest run --config vitest.config.ts tests/e2e",
    "test:coverage": "vitest run --config vitest.config.ts --coverage",
    "test:ci": "vitest run --config vitest.config.ts --coverage --reporter=json --outputFile=test-results.json",
    "migrate:gen": "drizzle-kit generate:pg",
    "migrate:up": "drizzle-kit up:pg",
    "migrate:down": "drizzle-kit down:pg",
    "migrate:status": "drizzle-kit check:pg",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx scripts/seed-database.ts",
    "db:reset": "npm run migrate:down && npm run migrate:up && npm run db:seed",
    "docker:build": "docker build -t scheduler-domain .",
    "docker:run": "docker run -p 3080:3080 --env-file .env scheduler-domain",
    "docker:push": "docker tag scheduler-domain registry.valero-neuroerp.com/scheduler-domain:latest && docker push registry.valero-neuroerp.com/scheduler-domain:latest",
    "ci:build": "npm run clean && npm run lint && npm run test:ci && npm run build",
    "ci:deploy": "npm run docker:build && npm run docker:push",
    "prepublishOnly": "npm run ci:build",
    "health:check": "curl -f http://localhost:3080/health || exit 1",
    "ready:check": "curl -f http://localhost:3080/ready || exit 1",
    "smoke:test": "npm run health:check && npm run ready:check",
    "performance:test": "artillery run tests/performance/load-test.yml",
    "security:audit": "npm audit --audit-level high",
    "security:scan": "snyk test",
    "docs:generate": "typedoc --out docs src",
    "docs:serve": "npx http-server docs -p 3007"
  },
  "dependencies": {
    "@valero-neuroerp/contracts": "file:../shared/contracts",
    "@valero-neuroerp/data-models": "file:../data-models",
    "@valero-neuroerp/utilities": "file:../utilities",
    "fastify": "^4.28.1",
    "@fastify/swagger": "^8.0.0",
    "@fastify/swagger-ui": "^4.0.0",
    "@fastify/cors": "^9.0.0",
    "@fastify/rate-limit": "^9.0.0",
    "@fastify/helmet": "^11.0.0",
    "zod": "^3.23.8",
    "zod-openapi": "^0.9.0",
    "drizzle-orm": "^0.31.0",
    "postgres": "^3.4.0",
    "pg": "^8.11.0",
    "nats": "^2.18.0",
    "jose": "^5.2.0",
    "pino": "^9.3.0",
    "ioredis": "^5.4.1",
    "uuid": "^10.0.0",
    "dotenv": "^17.2.3",
    "rrule": "^2.8.1",
    "cron-parser": "^4.9.0",
    "node-cron": "^3.0.3",
    "crypto": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "^24.6.1",
    "@types/pg": "^8.15.5",
    "@types/uuid": "^10.0.0",
    "@types/node-cron": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.3",
    "vitest": "^1.5.0",
    "supertest": "^7.1.4",
    "@vitest/coverage-v8": "^1.5.0",
    "drizzle-kit": "^0.22.0",
    "@types/supertest": "^6.0.0",
    "rimraf": "^5.0.5",
    "prettier": "^3.3.0",
    "eslint": "^8.57.0",
    "typedoc": "^0.26.0",
    "artillery": "^2.0.8",
    "snyk": "^1.1290.0",
    "http-server": "^14.1.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "volta": {
    "node": "18.19.0",
    "npm": "9.6.7"
  }
}