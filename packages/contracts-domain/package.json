{
  "name": "@valero-neuroerp/contracts-domain",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "tsx watch src/app/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.cjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "migrate:gen": "drizzle-kit generate:pg",
    "migrate:up": "drizzle-kit up:pg",
    "migrate:down": "drizzle-kit down:pg",
    "lint": "echo 'lint placeholder'",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@valero-neuroerp/contracts": "file:../shared/contracts",
    "@valero-neuroerp/data-models": "file:../data-models",
    "@valero-neuroerp/utilities": "file:../utilities",
    "fastify": "^4.28.1",
    "@fastify/swagger": "^8.0.0",
    "@fastify/swagger-ui": "^4.0.0",
    "zod": "^3.23.8",
    "zod-openapi": "^0.9.0",
    "drizzle-orm": "^0.31.0",
    "postgres": "^3.4.0",
    "pg": "^8.11.0",
    "nats": "^2.18.0",
    "jose": "^5.2.0",
    "pino": "^9.3.0",
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/sdk-node": "^0.51.0",
    "@opentelemetry/sdk-trace-base": "^1.25.0",
    "@opentelemetry/exporter-otlp-http": "^0.51.0",
    "@opentelemetry/instrumentation": "^0.51.0",
    "@opentelemetry/instrumentation-http": "^0.51.0",
    "@opentelemetry/instrumentation-pg": "^0.40.0",
    "@opentelemetry/instrumentation-ioredis": "^0.39.0",
    "@opentelemetry/semantic-conventions": "^1.25.0",
    "@opentelemetry/resources": "^1.25.0",
    "ioredis": "^5.4.1",
    "uuid": "^10.0.0",
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "@types/node": "^24.6.1",
    "@types/pg": "^8.15.5",
    "@types/uuid": "^10.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.3",
    "vitest": "^1.5.0",
    "supertest": "^7.1.4",
    "@vitest/coverage-v8": "^1.5.0",
    "drizzle-kit": "^0.22.0",
    "@types/supertest": "^6.0.0"
  }
}