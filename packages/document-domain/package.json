{
  "name": "@valero-neuroerp/document-domain",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "tsx watch src/app/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.cjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "migrate:gen": "drizzle-kit generate",
    "migrate:push": "drizzle-kit push",
    "migrate:drop": "drizzle-kit drop",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@valero-neuroerp/contracts": "file:../shared/contracts",
    "@valero-neuroerp/data-models": "file:../data-models",
    "@valero-neuroerp/utilities": "file:../utilities",
    "fastify": "^4.28.1",
    "@fastify/swagger": "^8.0.0",
    "@fastify/swagger-ui": "^4.0.0",
    "@fastify/multipart": "^8.0.0",
    "zod": "^3.23.8",
    "zod-openapi": "^0.9.0",
    "drizzle-orm": "^0.31.0",
    "postgres": "^3.4.0",
    "pg": "^8.11.0",
    "nats": "^2.18.0",
    "jose": "^5.2.0",
    "pino": "^9.3.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-trace-node": "^1.26.0",
    "@opentelemetry/sdk-trace-base": "^1.26.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.53.0",
    "@opentelemetry/resources": "^1.26.0",
    "@opentelemetry/semantic-conventions": "^1.27.0",
    "@opentelemetry/auto-instrumentations-node": "^0.50.0",
    "ioredis": "^5.4.1",
    "uuid": "^10.0.0",
    "dotenv": "^17.2.3",
    "handlebars": "^4.7.8",
    "nanoid": "^5.0.7",
    "axios": "^1.6.0",
    "pdf-lib": "^1.17.1",
    "@aws-sdk/client-s3": "^3.470.0",
    "@aws-sdk/s3-request-presigner": "^3.470.0"
  },
  "devDependencies": {
    "@types/node": "^24.6.1",
    "@types/pg": "^8.15.5",
    "@types/uuid": "^10.0.0",
    "@types/handlebars": "^4.1.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.5",
    "tsx": "^4.19.0",
    "typescript": "^5.6.3",
    "vitest": "^1.5.0",
    "supertest": "^7.1.4",
    "@vitest/coverage-v8": "^1.5.0",
    "drizzle-kit": "^0.22.0",
    "@types/supertest": "^6.0.0",
    "playwright": "^1.48.2"
  }
}
